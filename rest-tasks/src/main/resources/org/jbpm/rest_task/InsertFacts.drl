package org.jbpm.rest_task;

rule "Hello World"
	
	when
		$s : String()
	then
		System.out.println( "Hello World, " + $s);
end


rule "Hello Group 1"
	ruleflow-group "ruleflow1"
	
	when
	then
		System.out.println( "Hello from ruleflow1" );
end


rule "Hello Group 2"
	ruleflow-group "ruleflow2"

	when
	then
		System.out.println( "Hello from ruleflow2" );
end
